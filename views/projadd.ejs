  <body>
    <% include ../views/layouttopo %>
    <% include ../views/layoutmenu %>
    </br>
    <div class="row justify-content-around bg-dark" style="margin:1px;">
      <div style="position:relative;"><img src="/images/projeto.png" class="rounded float-left"></div>
      <!-- formulario -->
      <form action="/proj/add" method="POST" id ="formi" class="bg-dark rounded">
          </br></br></br>
          <div class="form-group d-flex justify-content-end">
            <label for="pnome" style="color:white;"><h4>Nome do Proj:&nbsp</h4></label>
            <input type="text" name="pnome" class="form-control col-md-6">
          </div>
          <div class="form-group d-flex justify-content-end">
            <label for="pdesc" style="color:white;"><h4>Descrição:&nbsp</h4></label>
            <input type="text" class="form-control col-md-6" name="pdesc">
          </div>
          <div class="form-group d-flex justify-content-end">
            <label for="pimg" style="color:white;"><h4>Link da Imagem:&nbsp</h4></label>
            <input type="text" class="form-control col-md-6" name="pimg">
          </div>
          <div class="form-group d-flex justify-content-end">
            <label for="plink" style="color:white;"><h4>Github:&nbsp</h4></label>
            <input type="text" class="form-control col-md-6" name="plink">
          </div>
          <div class="form-group d-flex justify-content-end">
            <label for="dnome" style="color:white;"><h4>Desenvolvedor:&nbsp</h4></label>
            <input type="text" class="form-control col-md-6" name="dnome">
          </div>
          <div class="form-group d-flex justify-content-end">
            <label for="linguagem" style="color:white;"><h4>Linguagem:&nbsp</h4></label>
            <input type="text" class="form-control col-md-6" name="linguagem">
          </div>
          <button type="submit" class="btn btn-outline-light float-right">Enviar</button>
        </form>
    </div>
    </br>
    <% include ../views/rodape %>
    <!-- Script js jquery -->
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
